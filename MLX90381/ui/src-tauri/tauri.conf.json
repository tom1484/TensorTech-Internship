{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "MLX90381_UI",
  "version": "1.0.0",
  "identifier": "com.tensortech.mlx90381-ui",
  "build": {
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "MLX90381 UART UI",
        "width": 1200,
        "height": 850,
        "minWidth": 900,
        "minHeight": 700,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}

